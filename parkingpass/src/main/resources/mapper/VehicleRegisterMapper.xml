<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.parkingpass.mapper.VehicleRegisterMapper">
    <insert id="addVehicleRegisterByUserId" parameterType="Vehicle_Register" >
       INSERT  INTO  vehicle_register(licence_plate_number,reid,uid)
       VALUE (#{licence_plate_number},#{reid},#{uid})
    </insert>

</mapper>